{
    "name": "bencates/php_as_expected",
    "description": "Expect-style assertions for PHP.",
    "authors": [
        {
            "name": "Ben Cates",
            "email": "ben.cates@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mustache/mustache": "2.1.*"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-0": { "PHPAsExpected": "src/" },
        "files": [
            "src/PHPAsExpected/Expectations/register.php",
            "src/PHPAsExpected/functions.php"
        ]
    }
}
